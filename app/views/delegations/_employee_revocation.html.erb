<div class="alert alert-danger alert-dismissible" role="alert">
	<button type="button" class="close" data-dismiss="alert">
	  <span aria-hidden="true">&times;</span>
	  <span class="sr-only">close</span>
	</button>
	<%= 'Your account is currently managed by ' + current_user.manager_delegation.manager.full_identity + '. ' %>
	<%= link_to 'click here to revoke it', delegation_revoke_path(current_user.manager_delegation.token), class: 'btn btn-hollow' %>
</div>